#include "stm32f767xx.h"
#include "stm32f767xx_spi_driver.h"

/*
 * ALT Function mode 5 (AF5)
 *
 * PB12 --> SPI2_NSS
 * PB13 --> SPI2_SCK
 * PB14 --> SPI2_MISO
 * PB15 --> SPI2_MOSI
 */

int main(void){

	// this function is used to initialize the GPIO pins to behave like SPI2 pins (alternate functions)
	SPI_GPIOInits();

	return 0;
}
